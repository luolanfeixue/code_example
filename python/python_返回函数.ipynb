    "from functools import reduce"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "def calc_prod(lst):\n",
    "    def lazy_prod():\n",
    "        def f(x,y):\n",
    "            return x*y\n",
    "        return reduce(f,lst,1)\n",
    "    return lazy_prod"
   ]
  },
